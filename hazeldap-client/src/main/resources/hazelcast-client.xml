<?xml version="1.0" encoding="UTF-8"?>
<hazelcast-client xsi:schemaLocation="http://www.hazelcast.com/schema/client-config hazelcast-client-config-3.12.xsd"
                  xmlns="http://www.hazelcast.com/schema/client-config"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <properties>
    <property name="hazelcast.client.statistics.enabled">true</property>
    <!--<property name="hazelcast.application.validation.token">MyToken334</property>-->
    <property name="hazelcast.client.heartbeat.timeout">1000</property>
    <property name="hazelcast.client.heartbeat.interval">200</property>
  </properties>


  <group>
    <name>hazeldap</name>
    <password>hazeldap-password-ignored-check</password>
  </group>

  <network>
    <cluster-members>
      <!-- Configure the configuration to be made to localhost only -->
      <address>127.0.0.1:5901</address>
      <address>127.0.0.1:5902</address>
    </cluster-members>


  </network>


</hazelcast-client>